{
  "name": "api",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "start:dev": "yarn run prepare:dev && nest start --watch",
    "build": "nest build",
    "docker:build": "docker build . -t jsxmail-api",
    "start": "node dist/main.js",
    "lint": "eslint \"{src}/**/*.ts\" --fix",
    "prepare:dev": "yarn jsxm prepare && yarn run docker:up && prisma migrate dev && prisma generate && prisma db seed",
    "prepare:prod": "yarn jsxm prepare && prisma migrate deploy && prisma generate && prisma db seed",
    "docker:up": "docker compose up -d"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.515.0",
    "@aws-sdk/client-ses": "^3.758.0",
    "@aws-sdk/client-sesv2": "^3.758.0",
    "@jsx-mail/core": "^2.2.2",
    "@nestjs/bullmq": "^10.2.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.3.1",
    "@prisma/client": "^5.9.1",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "bullmq": "^5.15.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "country-to-currency": "^1.1.5",
    "dotenv": "^16.4.4",
    "handlebars": "^4.7.8",
    "imagemin-pngquant": "^9.0.2",
    "jsx-mail": "^2.3.2",
    "moment": "^2.30.1",
    "multer": "1.4.5-lts.1",
    "nestjs-fingerprint": "^1.0.4",
    "nestjs-real-ip": "^3.0.1",
    "nodemailer": "^6.10.0",
    "prisma-soft-delete-middleware": "^1.3.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "stripe": "^14.15.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^6.4.17",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "prisma": "^5.9.0",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed/index.ts"
  }
}
